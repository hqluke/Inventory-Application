<html lang="en">
    <head>
        <title>Food Page</title>
    </head>
    <body>
        <h1>Welcome to the food page</h1>
        <% if (foods.length > 0) { %>
        <ul>
            <% foods.forEach(food => { %>

            <li><%= food.name %></li>
            <% }) %>
        </ul>
        <% } else { %>
        <p>No foods found</p>
        <% } %>
        <p>Don't see your food? <a href="/food/add">Add it here</a></p>
        <a href="/">Back</a>
    </body>
</html>
